    {
      "name": "memory",
      "displayName": "Session Memory Manager",
      "version": "2.1.0",
      "description": "Track tool usage and extract session memories at configurable thresholds for enhanced context continuity",
      "longDescription": "The memory plugin automatically monitors your Claude Code sessions, tracking tool usage patterns, conversation topics, and important decisions. It extracts and summarizes key information at configurable intervals, maintaining persistent memory across sessions. Features include automatic context compression, intelligent memory triggers, topic clustering, and seamless integration with your workflow. Essential for long-term projects and complex development sessions.",

      "author": {
        "name": "Kierr",
        "email": "kierr@example.com",
        "url": "https://github.com/kierr",
        "organization": "Claude Code Community"
      },

      "license": "Apache-2.0",
      "homepage": "https://github.com/kierr/claude-memory",
      "repository": {
        "type": "git",
        "url": "https://github.com/kierr/claude-memory.git"
      },

      "source": {
        "source": "github",
        "repo": "kierr/claude-memory"
      },

      "keywords": [
        "memory",
        "context",
        "tracking",
        "automation",
        "persistence",
        "continuity"
      ],

      "category": "Productivity",
      "subcategory": "Automation",

      "tags": {
        "primary": ["memory", "automation"],
        "secondary": ["context", "tracking", "persistence"],
        "stack": ["typescript", "nodejs"],
        "difficulty": "intermediate",
        "use-case": [
          "long-term-projects",
          "complex-sessions",
          "context-management"
        ]
      },

      "components": {
        "commands": [
          {
            "name": "memory-status",
            "description": "Show current memory status and statistics",
            "usage": "/memory-status",
            "examples": ["/memory-status", "/memory-status --detailed"]
          },
          {
            "name": "memory-extract",
            "description": "Manually extract session memories",
            "usage": "/memory-extract [options]",
            "examples": [
              "/memory-extract",
              "/memory-extract --since 2h",
              "/memory-extract --topic architecture"
            ]
          },
          {
            "name": "memory-config",
            "description": "Configure memory extraction settings",
            "usage": "/memory-config [setting] [value]",
            "examples": [
              "/memory-config threshold 100",
              "/memory-config auto-extract true",
              "/memory-config show"
            ]
          },
          {
            "name": "memory-search",
            "description": "Search through session memories",
            "usage": "/memory-search <query>",
            "examples": [
              "/memory-search database schema",
              "/memory-search --today api decisions",
              "/memory-search --topic debugging"
            ]
          }
        ],
        "agents": [
          {
            "name": "memory-analyzer",
            "description": "AI agent for intelligent memory extraction and analysis",
            "capabilities": [
              "pattern-recognition",
              "topic-clustering",
              "importance-scoring",
              "context-summarization"
            ],
            "model": "claude-sonnet-4-5",
            "tools": ["read", "write", "bash", "grep"]
          }
        ]
      },

      "installation": {
        "type": "automatic",
        "steps": [
          "Set up memory storage directory",
          "Configure extraction thresholds",
          "Initialize tracking system",
          "Test memory extraction"
        ],
        "configuration": {
          "required": ["MEMORY_DIR"],
          "optional": ["EXTRACT_THRESHOLD", "MAX_MEMORY_SIZE", "AUTO_COMPRESS"],
          "environment": [".env", "memory-config.json"]
        }
